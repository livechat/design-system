{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build:lib",
          "build:styles",
          "build:icons",
          "prebuild",
          "lint",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["{projectRoot}/dist", "{projectRoot}/lib"]
    },
    "build:lib": {
      "outputs": ["{projectRoot}/dist", "{projectRoot}/lib"]
    },
    "build:styles": {
      "outputs": ["{projectRoot}/dist"]
    },
    "prebuild": {
      "outputs": ["{projectRoot}/lib", "{projectRoot}/entryConfig.ts"]
    },
    "build:icons": {
      "outputs": ["{projectRoot}/dist", "{projectRoot}/lib"]
    },
    "test": {
      "dependsOn": ["^build"]
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [],
    "production": ["default"]
  }
}
