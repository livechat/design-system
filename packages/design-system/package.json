{
  "name": "@livechat/design-system",
  "version": "0.13.4",
  "publishConfig": {
    "access": "public"
  },
  "description": "LiveChat Design System React components with css styles",
  "homepage": "https://livechat.github.io/design-system/",
  "repository": {
    "type": "git",
    "url": "https://github.com/livechat/design-system"
  },
  "typings": "src/interfaces/index.d.ts",
  "main": "dist/design-system.cjs.js",
  "module": "dist/design-system.es.js",
  "jsnext:main": "dist/design-system.es.js",
  "style": "dist/design-system.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c",
    "prebuild": "rimraf dist",
    "build:cdn": "npm run build && mkdir -p ../../dist/cdn && cp dist/design-system.css ../../dist/cdn/styles.css && cp ../../dist/cdn/styles.css ../../dist/cdn/styles-v$npm_package_version.css",
    "prepare": "npm run build",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/prop-types": "^15.5.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "autoprefixer": "^9.0.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-css-modules-transform": "^1.6.1",
